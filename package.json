{
  "name": "opennem",
  "version": "4.51.0",
  "description": "An Open Platform for National Electricity Market Data",
  "author": "Steven Tan",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/nuxt",
    "build": "nuxt build",
    "build-export": "nuxt build && nuxt export",
    "start": "./node_modules/.bin/nuxt start",
    "serve": "nuxt serve",
    "generate": "FOR=prod nuxt generate",
    "generate-dev": "FOR=dev nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "release": "dotenv release-it --",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint --fix \"**/*.*\"",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:dev": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:exec": "docker-compose exec opennem-fe",
    "docker:logs": "docker-compose logs -f opennem-fe",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/proxy": "^2.0.1",
    "@opennem/energy-tools": "^1.5.1",
    "@turf/length": "^6.3.0",
    "cross-env": "^5.2.0",
    "d3-array": "^2.0.3",
    "d3-axis": "^1.0.12",
    "d3-brush": "^1.0.6",
    "d3-color": "^3.1.0",
    "d3-format": "^1.3.2",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^2.1.2",
    "d3-scale-chromatic": "^1.3.3",
    "d3-selection": "^1.3.2",
    "d3-shape": "^1.2.2",
    "d3-time": "^1.0.10",
    "d3-time-format": "^2.1.3",
    "d3-zoom": "^1.7.3",
    "date-fns": "^2.14.0",
    "date-fns-tz": "^1.1.3",
    "file-saver": "^2.0.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.includes": "^4.3.0",
    "lodash.isempty": "^4.4.0",
    "lodash.orderby": "^4.6.0",
    "lodash.remove": "^4.7.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "mapbox-gl": "3.2.0",
    "mapbox-gl-animated-popup": "0.2.1",
    "nuxt": "^2.15.8",
    "papaparse": "^5.3.1",
    "v-dragged": "^0.0.5",
    "v-tooltip": "^2.0.2",
    "vue-clickaway": "^2.2.2",
    "vue-json-csv": "^1.2.4",
    "vue-mapbox": "^0.4.1",
    "vueperslides": "^2.13.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "bulma": "^0.8.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^6.0.1",
    "nodemon": "^1.11.0",
    "prettier": "^2.7.1",
    "release-it": "^14.14.3",
    "sass-loader": "^10.2.1"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
